{
    "name": "i-lateral/profanity-filter",
    "description": "Simple library for detecting swearwords in a string and returning an error if detected",
    "keywords": [
        "PHP",
        "profanity",
        "swear words"
    ],
    "type": "library",
    "homepage": "https://github.com/i-lateral/profanity-filter",
	"license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "homepage": "http://ilateral.co.uk"
        }
    ],
	"support": {
		"issues": "http://github.com/i-lateral/profanity-filter/issues"
	},
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "ProfanityFilter\\": "src/",
            "ProfanityFilter\\Tests\\": "tests/"
        }
    }
}